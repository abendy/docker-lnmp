{% if items is defined and items is not empty %}
  {% for neighborhood, items in items %}
  <section class="module content-list grouped-content-list">

    {% if neighborhood is defined and neighborhood is not empty %}
    <div class="content-list__top">
      <h2 class="content-list__title">{{ neighborhood }}</h2>
    </div>
    {% endif %}

    {% block content_list_item %}
    {% include 'partial/content-list-item.twig' %}
    {% endblock %}

  </section>
  {% endfor %}
{% endif %}
